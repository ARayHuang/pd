    "test": "npm run test:e2e && npm run test:backend",
    "ncu": "node_modules/npm-check-updates/bin/ncu.js",
    "server:webpack": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.development.config.js --hot",
    "dev": "concurrently \" npm run start\" \"npm run server:webpack\"",
    "build:production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --config webpack.production.config.js",
    "build:alpha": "NODE_ENV=alpha node_modules/webpack/bin/webpack.js --config webpack.production.config.js",
    "setup:alpha": "ansible-playbook ansible/setup-server.yml -e host=pd-alpha -e env=alpha",
    "deploy:alpha": "ansible-playbook ansible/build.yml && ansible-playbook ansible/deploy.yml -e host=pd-alpha -e env=alpha",
    "changelog": "node_modules/conventional-changelog-cli/cli.js -n changelog.config.js",
    "test:e2e": "jest -c ./tests/e2e/jest.config.js",
    "test:backend": "jest --forceExit -c ./tests/backend/jest.config.js",
    "ci": "start-server-and-test \"3000|3001|3003\""
  },
